fibonacci <- function(n) {
if (n <= 0) {
return(numeric(0))
}
if (n == 1) {
return(c(0))
}
fib_sequence <- c(0, 1)
while (length(fib_sequence) < n) {
last_two <- tail(fib_sequence, 2)
next_fib <- last_two[1] + last_two[2]
fib_sequence <- c(fib_sequence, next_fib)
}
return(fib_sequence)
}
num_fibs <- 10
result <- fibonacci(num_fibs)
cat("The first", num_fibs, "Fibonacci numbers are:\n")
print(result)
#4. Write a R Program to Sample data from a Population.
set.seed(123)
population <- 1:1000
sample_size <- 50
sampled_data <- sample(population, size = sample_size, replace = FALSE)
cat("Original Population (first 20 elements):\n")
print(head(population, 20))
cat("\nTotal population size:", length(population), "\n\n")
cat("Randomly Sampled Data:\n")
print(sampled_data)
cat("\nSample size:", length(sampled_data), "\n")
find_primes <- function(n) {
if (n < 2) {
return(numeric(0))
}
is_prime <- rep(TRUE, n + 1)
is_prime[1] <- FALSE
is_prime[2] <- FALSE
for (p in 2:sqrt(n)) {
if (is_prime[p]) {
for (i in seq(p*p, n, by = p)) {
is_prime[i] <- FALSE
}
}
}
primes <- which(is_prime)
return(primes)
}
number_limit <- 50
prime_numbers <- find_primes(number_limit)
cat("Prime numbers up to", number_limit, "are:\n")
print(prime_numbers)
